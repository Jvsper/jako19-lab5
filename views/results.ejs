<%- include ('partials/header.ejs') %>

	<form action="/search" method="GET">
		<input type="text" placeholder="<%=keyword %>" name="keyword"/>
		<button>
		 	Search
		</button> <br>
		<a id="favLink" href="/displayKeywords"> See Favorites </a>
	</form>

<% imageURLs.forEach(function(imageURL, i){ %>
	<%- (i%3==0)?'<br>':''%>
	<div class="imageContainer">
	<img class="image"src="<%=imageURL%>" width="200" height="200" alt="imageURL">
	<img class="favoriteIcon" src="img/favorite.png" width="20" alt="favorite off">
	<div>
<% }) %>

<%- include ('partials/footer.ejs') %>